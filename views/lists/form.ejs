<form action="<%= url %>" method="post">
  <div class="mb-3">
    <label for="title" class="form-label">Title</label>
    <input type="text" name='title' value='<%= list.title %>' class="form-control" id="title" placeholder="name@example.com">
  </div>
  <div class="mb-3">
    <label for="description" class="form-label">Description</label>
    <textarea class="form-control" name='description' id="description" rows="3"><%= list.description %></textarea>
  </div>

  <button type="submit" class="btn btn-primary">Save</button>
</form>